<body class="mainstream">

    {% block page_header %}
    <!-- begin default page header -->
    <script type="text/javascript">document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
    <div id="skiplink-container">
        <div>
            <a href="#content" class="skiplink">Skip to main content</a>
        </div>
    </div>
    <header role="banner" id="global-header" class=""><div class="header-wrapper">
        <div class="header-global">
            <div class="header-logo">
                <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" id="logo" class="content">
                    <img src="https://assets.digital.cabinet-office.gov.uk/static/gov.uk_logotype_crown-c09acb07e4d1d5d558f5a0bc53e9e36d.png" alt=""> GOV.UK
                </a>
            </div>

            {% block header_search %}
            <a href="#search" class="search-toggle js-header-toggle">Search</a>
            <form id="search" class="site-search" action="/search" method="get" role="search">
                <div class="content">
                    <label for="site-search-text">Search</label>
                    <input type="search" name="q" id="site-search-text" title="Search" class="js-search-focus">
                    <input class="submit" type="submit" value="Search">
                </div>
            </form>
            {% endblock %}
        </div>
    </header>
    <div id="global-cookie-message">
        <p>
            GOV.UK uses cookies to make the site simpler.
            <a href="https://www.gov.uk/help/cookies">Find out more about cookies</a>
        </p>
    </div>
    <!-- end default page header -->
    {% endblock %}

    
    <div id="alphaMessage">
        <p>This website is in <strong>ALPHA</strong> and all content is subject to change.</p>
    </div>
    {% if ft_config['hide_breadcrumb'] is not defined or ft_config['hide_breadcrumb'] == false %}
    {{ block('bc') }}
    {% endif %}

    <div id="wrapper" class="answer smart_answer directgov">


{% block body %}
    <main id="content" role="main">

    <div id="js-replaceable">

        {% set _title = block('title') %}
        {% if _title is not empty %}
        <header class="page-header group"><div>
            <h1>{{ block('title') }}</h1>
          </div>
        </header>
        {% endif %}

        <div class="article-container group">

            {% for flashMessage in app.session.flashbag.get('notice') %}
                <div class="flash-message">
                    <em>Notice</em>: {{ flashMessage }}
                </div>
            {% endfor %}

            <article role="article" class="group">
                <div class="inner">
                    {% block content %}{% endblock %}
                </div>
            </article>
        </div>
    </div>


    {% block meta %}
    <div class="meta-wrapper">
        <!-- report_a_problem -->
        <p class="report-a-problem-toggle js-footer"><a href="">Is there anything wrong with this page?</a></p>
        <div class="report-a-problem-container">
            <h2>Help us improve GOV.UK</h2>
            <form accept-charset="UTF-8" action="/contact/govuk/problem_reports" method="post">
            <div style="margin:0;padding:0;display:inline"><input name="utf8" type="hidden" value="✓"></div>
            <input id="url" name="url" type="hidden" value="https://www.gov.uk/report-a-lost-or-stolen-passport"><input id="source" name="source" type="hidden" value=""><input id="page_owner" name="page_owner" type="hidden" value="">
            <p>Please don't include any personal or financial information, for example your National Insurance or credit card numbers.</p>
            <label for="what_doing">What you were doing</label>
            <input id="what_doing" name="what_doing" type="text"><label for="what_wrong">What went wrong</label>
            <input id="what_wrong" name="what_wrong" type="text"><div><button name="commit" type="submit" class="button">Send</button></div>
            <!--<p>If you have more extensive feedback, please visit the <a href="/feedback">support page</a>.</p>-->
          </form>
        </div>
        <!-- end report_a_problem -->
        <div class="meta-data group">
            <div class="inner">
                <div class="print-and-modified-date group">
                    <p class="modified-date">Last updated: 13 February 2014</p>
                </div>
            </div>
        </div>
    </div>
    {% endblock %}
</main>


{% if ft_config['hide_sidebar'] is not defined or ft_config['hide_sidebar'] == false %}
{{ block('sb') }}
{% endif %}

</div><!-- end wrapper -->


